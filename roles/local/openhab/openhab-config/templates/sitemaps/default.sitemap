          // Switch item=AlarmLowBattDoorSide_garage label="Deursensor Garagezijdeur []" icon="battery" mappings=[ON="Yes",OFF="No"]
          // Switch item=AlarmLowBattDoor_living label="Deursensor Tuindeur []" icon="battery" mappings=[ON="Yes",OFF="No"]
          // Switch item=AlarmLowBattDoor_laundry label="Deursensor Washok []" icon="battery" mappings=[ON="Yes",OFF="No"]
          // Switch item=AlarmLowBattSiren_kitchen label="Alarm Keuken []" icon="battery" mappings=[ON="Yes",OFF="No"]
          // Switch item=AlarmLowBattSmoke_garage label="Rookmelder Garage []" icon="battery" mappings=[ON="Yes",OFF="No"]
        // }
    }

