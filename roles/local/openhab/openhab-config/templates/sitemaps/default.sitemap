    Frame icon="firstfloor" label="Verlichting boven" {
        Switch item=Scene_corridor    label="Overloop"       mappings=[OFF="UIT", EVENING="AVOND", WORK="WERK",  BRIGHT="MAX"]
        Switch item=Scene_bedroom     label="Slaapkamer"     mappings=[OFF="UIT", EVENING="AVOND", READ="LEES",  COSY="KNUS"]
        Switch item=Scene_office      label="Kantoor"        mappings=[OFF="UIT", EVENING="AVOND", READ="LEES",  BRIGHT="MAX"]
        Switch item=Scene_laundry     label="Fitness"        mappings=[OFF="UIT", EVENING="AVOND", WORK="WERK",  BRIGHT="MAX"]
        Switch item=Scene_bathroom    label="Badkamer"       mappings=[OFF="UIT", EVENING="AVOND", READ="LEES",  BRIGHT="MAX"]
        Switch item=Scene_menno       label="Menno's kamer"  mappings=[OFF="UIT", EVENING="AVOND", READ="LEES",  BRIGHT="MAX"]
