    Frame icon="garden" label="Verlichting buiten" {
        Switch item=Scene_outdoor   label="Buiten"  mappings=[OFF="UIT", EVENING="AVOND", WORK="WERK",  BRIGHT="MAX"]
        // Group item=gScene_outdoor icon="lightbulb" label="Buitenverlichting individueel"
