    Frame label="Energy" {
        Text item=Power_Delta_All_Current {
            Text item=Power_Delta_All_Current
            Text item=Power_Delta_L1_Current
            Text item=Power_Delta_L2_Current
            Text item=Power_Delta_L3_Current
        }
        Text item=Solar_Prod_Current
        Text item=Power_Delta_Hour {
            Text item=Power_Use_Hour_Cost
            Text item=Power_Ret_Hour_Cost
            Text item=Power_Use_Total icon="pressure"
            Text item=Power_Ret_Total icon="pressure"
        }
        Text item=Gas_Use_Hour
        Text item=Gas_Use_Total icon="pressure"
    }

    Frame label="Verwarming" {
        Text item=HeatTemp_living valuecolor=[>=22="red",>18="green",<=18="orange"]
        Text item=HeatSetPoint {
            Text item=HeatNextPoint
            Text item=Heatmode
            Text item=HeatPressure
            Text item=HeatWaterTemp
            Text item=HeatStarts
            Text item=HeatBurnTime
        }
        Text item=Heatfill
        Text item=Heatmaintenance
    }

    Frame label="Airco" {
        Text item=HeatTemp_living label="Woonkamer" icon="snow" valuecolor=[>22="red",>=19="green",<19="oranje"] visibility=[hvacPower_living==ON]
        Text item=HeatTemp_living label="Woonkamer" icon="switch-off" valuecolor=[>22="red",>=19="green",<19="oranje"] visibility=[hvacPower_living==OFF]
          Text item=hvacPower_living label="Airco [%s]" icon="snow" {
            Switch item=hvacPower_living
            Setpoint item=hvacSetTemp_living minValue=18 maxValue=24 step=1
            Switch item=hvacMode_living label="Mode" mappings=["8"="Auto", "3"="Koud", "1"="Warm"]
            Switch item=hvacFanSpeed_living label="Fan " mappings=["0"="Auto", "1"="Stil", "2"="1", "3"="2", "4"="3"]
            Switch item=hvacVaneHor_living label="Vane H" mappings=["0"="Auto", "12"="Swing", "1"="1", "3"="3", "5"="5"]
            Switch item=hvacVaneVert_living label="Vane V" mappings=["0"="Auto", "12"="Swing", "1"="1", "3"="3", "5"="5"]
            Switch item=hvacVacation_living
          }

        Text item=hvacTemp_bedroom label="Slaapkamer" icon="snow" visibility=[hvacPower_bedroom==ON]
        Text item=hvacTemp_bedroom label="Slaapkamer" icon="switch-off" visibility=[hvacPower_bedroom==OFF]
          Text item=hvacPower_bedroom label="Airco [%s]" icon="snow" {
            Switch item=hvacPower_bedroom
            Setpoint item=hvacSetTemp_bedroom minValue=18 maxValue=23 step=1
            Switch item=hvacMode_bedroom label=" " mappings=["AUTO"="Auto", "COLD"="Koud", "HEAT"="Warm"]
            Switch item=hvacFanSpeed_bedroom label=" " mappings=["AUTO"="Auto", "SILENCE"="Stil", "LEVEL_2"="1", "LEVEL_3"="2", "LEVEL_5"="3"]
            Text item=hvacFanDirection_bedroom label=" "
            Switch item=hvacVacation_bedroom
          }
        Text item=hvacTemp_office icon="snow" visibility=[hvacPower_office==ON]
        Text item=hvacTemp_office icon="switch-off" visibility=[hvacPower_office==OFF]
          Text item=hvacPower_office label="Airco [%s]" icon="snow" {
            Switch item=hvacPower_office
            Setpoint item=hvacSetTemp_office minValue=18 maxValue=23 step=1
            Switch item=hvacMode_office label="" mappings=["AUTO"="Auto", "COLD"="Koud", "HEAT"="Warm"]
            Switch item=hvacFanSpeed_office label="" mappings=["AUTO"="Auto", "SILENCE"="Stil", "LEVEL_2"="1", "LEVEL_3"="2", "LEVEL_5"="3"]
            Text item=hvacFanDirection_office label=""
          }
        Text item=hvacTemp_fitnessroom icon="snow" visibility=[hvacPower_fitnessroom==ON]
        Text item=hvacTemp_fitnessroom icon="switch" visibility=[hvacPower_fitnessroom==OFF]
          Text item=hvacPower_fitnessroom label="Airco [%s]" icon="snow" {
            Switch item=hvacPower_fitnessroom
            Setpoint item=hvacSetTemp_fitnessroom minValue=18 maxValue=23 step=1
            Switch item=hvacMode_fitnessroom label="" mappings=["AUTO"="Auto", "COLD"="Koud", "HEAT"="Warm"]
            Switch item=hvacFanSpeed_fitnessroom label="" mappings=["AUTO"="Auto", "SILENCE"="Stil", "LEVEL_2"="1", "LEVEL_3"="2", "LEVEL_5"="3"]
            Text item=hvacFanDirection_fitnessroom label=""
          }
        Text item=hvacTemp_menno icon="snow" visibility=[hvacPower_menno==ON]
        Text item=hvacTemp_menno icon="switch" visibility=[hvacPower_menno==OFF]
          Text item=hvacPower_menno label="Airco status [%s]" icon="snow" {
            Switch item=hvacPower_menno
            Setpoint item=hvacSetTemp_menno minValue=18 maxValue=23 step=1
            Switch item=hvacMode_menno label="" mappings=["AUTO"="Auto", "COLD"="Koud", "HEAT"="Warm"]
            Switch item=hvacFanSpeed_menno label="" mappings=["AUTO"="Auto", "SILENCE"="Stil", "LEVEL_2"="1", "LEVEL_3"="2", "LEVEL_5"="3"]
            Text item=hvacFanDirection_menno label=""
          }
    }

    Frame label="Alarm" {
        Switch  item=AlarmStatus icon="alarm" label="Alarm []"  mappings=["DISARMED"="Disarm", "ARMED_HOME"="Arm Home", "ARMED_AWAY"="Arm Away"]
        Text item=gDoors {
            Text item=AlarmDoorSide_garage
            Text item=AlarmDoor_front
            Text item=AlarmDoor_laundry
            Text item=AlarmDoor_bedroom
            Text item=AlarmDoor_dining
            Text item=AlarmDoor_living
        }
    }

    Frame label="Christmas" {
        Switch  item=ShellyXmasTree_Switch
        Text    item=ShellyXmasTree_Power
        Switch  item=ShellyFireBin_Switch
        Text    item=ShellyFireBin_Power
        Switch  item=ShellyInfrared_Switch
        Text    item=ShellyInfrared_Power
        // Switch  item=ShellyXmasOutdoor_Switch
        // Text    item=ShellyXmasOutdoor_Power
        Switch  item=ShellyXmasChurch_Switch
        Text    item=ShellyXmasChurch_Power
