sitemap default label="At Home" {
    Frame label="Thuis" {
        Switch item=DayMode label="Dagfase []" icon="sun_clouds" mappings=[MORNING="Ochtend"]  visibility=[DayMode=="MORNING"]
        Switch item=DayMode label="Dagfase []" icon="sun_clouds" mappings=[DAY="Dag"]          visibility=[DayMode=="DAY"]
        Switch item=DayMode label="Dagfase []" icon="sun_clouds" mappings=[EVENING="Avond"]    visibility=[DayMode=="EVENING"]
        Switch item=DayMode label="Dagfase []" icon="sun_clouds" mappings=[NIGHT="Nacht"]      visibility=[DayMode=="NIGHT"]

        Text item=KNMI_Condition label="Weer [%s]" icon="sun_clouds" {
            Frame label="Buiten" {
                Text item=OutsideTemperature  label="Buitentemperatuur [%.0f %unit%]"
                Text item=KNMI_TempFeelStation
                Text item=KNMI_Humidity
                Text item=KNMI_DewPoint
                Text item=RainCurrent
                Text item=KNMI_Alarm         labelcolor=[>=3="red"] valuecolor=[0="green",1="gold",2="orange",3="red"]
                Text item=OWM_Current_WindDirection
                Text item=KNMI_WindForce
                Text item=Sunrise_Next
                Text item=Sunset_Next
                Text item=Astro_SunAzimuth    visibility=[Astro_SunElevation >= 0]
                Text item=Astro_SunElevation  visibility=[Astro_SunElevation >= 0]
                Text item=Astro_MoonPhase     visibility=[Astro_SunElevation < 0]
                Text item=Astro_MoonDistance  visibility=[Astro_SunElevation < 0]
                Text item=Astro_MoonFullNext
                Text item=Astro_MoonNewNext
                Text item=KNMI_Forcast
                Text item=OWM_Pollution_Quality_Index
                Text item=OWM_Pollution_PM25
                Text item=OWM_Pollution_Ozone_Current
                Text item=OWM_Pollution_NO2_Current
                Text item=OWM_Pollution_CO_Current
            }
            Frame label="Beganegrond" {
                Text item=LivingTemperature            valuecolor=[>=22="red",>18="green",<=18="orange"]
                Text item=AlarmTempSiren_kitchen       label="Keuken temperatuur [%.0f %unit%]"        valuecolor=[>=22="red",>18="green",<=18="orange"]
                Text item=MotionTemp_toilet            label="Toilet temperatuur [%.0f %unit%]"        valuecolor=[>=22="red",>=19="orange",<19="green"]
                Text item=AlarmTempSmoke_garage        label="Garage temperatuur [%.0f %unit%]"        valuecolor=[>22="red",>20="orange",<=20="green"]
                Text item=HeatHumidity_living          label="Luchtvochtigheid kamer [%.0f %unit%]"    valuecolor=[>65="orange",>40="green",<=40="red"]
                Text item=AlarmHumSmoke_garage         label="Luchtvochtigheid Garage [%.0f %unit%]"   valuecolor=[>65="orange",>40="green",<=40="red"]
            }
            Frame label="Boven" {
                Text item=AlarmTempSmoke_corridor      label="Overloop temperatuur [%.0f %unit%]"      valuecolor=[>22="red",>=18="green",<18="orange"]
                Text item=hvacTemp_bedroom             label="Slaapkamer temperatuur [%.0f %unit%]"    valuecolor=[>21="red",>18="orange",<=18="green"]
                Text item=hvacTemp_fitnessroom         label="Fitness temperatuur [%.0f %unit%]"       valuecolor=[>22="red",>18="orange",<=18="green"]
                Text item=hvacTemp_office              label="Kantoor temperatuur [%.0f %unit%]"       valuecolor=[>22="red",>20="orange",>10="green",<=10="blue"]
                Text item=hvacTemp_menno               label="Menno's kamer temperatuur [%.0f %unit%]" valuecolor=[>20="red",>18="orange",>10="green",<=10="blue"]
                Text item=AlarmHumSmoke_corridor       label="Luchtvochtigheid overloop"               valuecolor=[>60="orange",>40="green",<=40="red"]
            }
        }

        Text item=LivingTemperature   label="Woonkamer temperatuur [%.1f %unit%]"
        Text item=OutsideTemperature  label="Buitentemperatuur [%.0f %unit%]"
        Text item=RainCurrent         labelcolor=[>=3="red"] valuecolor=[<=0.1="green",<=1="gold",<=3="orange",>3="red"] visibility=[RainCurrent>2]
        Text item=KNMI_Forcast
        Text item=KNMI_Alarm          labelcolor=[>=3="red"] valuecolor=[0="green",1="gold",2="orange",3="red"]          visibility=[KNMI_Alarm>1]
        Text item=Sunrise_Next
        Text item=Sunset_Next
    }

    Frame label="Verlichting beneden" {
        Switch item=Scene_living         label="Woonkamer"       mappings=[OFF="UIT", EVENING="AVOND", MOVIE="FILM", READ="LEES"]
        Switch item=Scene_dining         label="Eethoek"         mappings=[OFF="UIT", EVENING="AVOND", READ="LEES", BRIGHT="MAX"]
        Switch item=Scene_kitchen        label="Keuken"          mappings=[OFF="UIT", EVENING="AVOND", WORK="WERK", BRIGHT="MAX"]
        Switch item=Scene_kitchenCeiling label="Keuken plafond"  mappings=[OFF="UIT", EVENING="AVOND", WORK="WERK", BRIGHT="MAX"]
        Switch item=Scene_hallway        label="Hal"             mappings=[OFF="UIT", EVENING="AVOND", READ="LEES", BRIGHT="MAX"]
        Switch item=Scene_hallwayCeiling label="Hal plafond"     mappings=[OFF="UIT", EVENING="AVOND", READ="LEES", BRIGHT="MAX"]
        // Switch item=Scene_garage      label="Garage"          icon="garage"   mappings=[OFF="UIT", EVENING="AVOND", WORK="WERK",  BRIGHT="MAX"]
